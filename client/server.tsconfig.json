{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "build",
        "baseUrl": ".",
        "typeRoots": [
            "./typing"
        ]
    },
    "include": [
        "server/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}